      </ImageView>
      <ImageView fitHeight="537.0" fitWidth="563.0" layoutY="232.0" preserveRatio="true">
         <image>
            <Image url="@../Image/erna.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="532.0" fitWidth="558.0" layoutX="916.0" layoutY="237.0" preserveRatio="true">
         <image>
            <Image url="@../Image/jonas.png" />
